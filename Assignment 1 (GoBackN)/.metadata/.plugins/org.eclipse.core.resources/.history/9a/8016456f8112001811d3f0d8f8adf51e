import java.util.Random;

public class GoBackN {

	static int packetSize = 12;
	static int windowSize = 6;
	static Random random;

	public static void main(String[] args){
		random = new Random();
		while(true){
			int[] pkt = generateRandomPacket();
			String packet = null;
			break;
			//sendPacket(packet); //sender 
			//receivePacket(packet); //receiver
			
		}
		
	}

	private static int[] generateRandomPacket() {
		int[] pkt = new int[packetSize];
		for(int i=0; i<packetSize; i++){
			pkt[i] = random.nextInt(9);
			System.out.print(pkt[i] + " ");
		}
		return pkt;
	}
	
}
